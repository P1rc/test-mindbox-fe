{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "react-app",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": ["react", "react-hooks", "@typescript-eslint", "jsx-a11y"],
  "rules": {
    "indent": ["error", 4],
    "quotes": ["error", "single"],
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}